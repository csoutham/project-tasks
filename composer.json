{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.4|^8.0",
    "blade-ui-kit/blade-heroicons": "^0.3.0",
    "calebporzio/awesome-helpers": "^2.4",
    "dyrynda/laravel-model-uuid": "^6.3",
    "fideloper/proxy": "^4.4",
    "fruitcake/laravel-cors": "^2.0",
    "genealabs/laravel-caffeine": "^8.0",
    "guzzlehttp/guzzle": "^7.0.1",
    "honeybadger-io/honeybadger-laravel": "^3.2",
    "laravel/framework": "^8.12",
    "laravel/horizon": "^5.4",
    "laravel/sanctum": "^2.8",
    "laravel/tinker": "^2.5",
    "livewire/livewire": "^2.0",
    "lukeraymonddowning/alpinimations": "^1.2",
    "owen-it/laravel-auditing": "^11.0",
    "predis/predis": "^1.1"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.5",
    "barryvdh/laravel-ide-helper": "^2.8",
    "beyondcode/helo-laravel": "^1.1",
    "facade/ignition": "^2.5",
    "fakerphp/faker": "^1.9.1",
    "laravel/breeze": "^0.0.5",
    "laravel/telescope": "^4.3",
    "mockery/mockery": "^1.4.2",
    "nunomaduro/collision": "^5.0",
    "phpunit/phpunit": "^9.3.3"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "dont-discover": [
        "laravel/telescope"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi",
      "@php artisan vendor:publish --force --tag=livewire:assets --ansi",
      "@php artisan ide-helper:generate"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi",
      "@php artisan ide-helper:generate"
    ],
    "post-update-cmd": [
      "@php artisan livewire:publish --assets",
      "@php artisan telescope:publish --ansi",
      "@php artisan horizon:publish --ansi",
      "@php artisan telescope:clear",
      "@php artisan view:clear",
      "@php artisan ide-helper:models --nowrite",
      "@php artisan test --tests=Feature"
    ]
  }
}
